<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="20.0"
   xmlns="http://javafx.com/javafx/8.0.171"
   xmlns:fx="http://javafx.com/fxml/1" fx:controller="dambat.KalkuController">
   <children>
      <Label prefHeight="35.0" prefWidth="559.0" text="Zenbaki biren arteko eragiketa gauzatzeko sakatu botoia."/>
      <Label prefHeight="27.0" prefWidth="489.0" text="Eragiketaz aldatzeko, sakatu eragiketa botoia."/>
      <HBox prefHeight="100.0" prefWidth="200.0" spacing="20.0">
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
         <children>
            <TextField fx:id="zenbakiBat" prefHeight="40.0" prefWidth="93.0" />
            <Button fx:id="ikurra" mnemonicParsing="false" text="+" onAction="#ikurraAldatu" />
            <TextField fx:id="zenbakiBi" prefHeight="40.0" prefWidth="93.0" />
            <Button mnemonicParsing="false" text="=" onAction="#emaitzaLortu" />
            <Label fx:id="emaitza" text="" prefHeight="41.0" prefWidth="107.0" />
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </HBox>
   </children>
</VBox>
